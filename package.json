    "angular-loader": "^1.7.5",
    "angular-route": "^1.7.5",
    "bootstrap": "^4.5.2",
    "css-loader": "^4.3.0",
    "html5-boilerplate": "0.0.1",
    "popper.js": "^1.16.1",
    "style-loader": "^1.2.1",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "angular-mocks": "^1.7.5",
    "angular-ui-grid": "^4.8.5",
    "babel-loader": "^8.1.0",
    "babel-plugin-angularjs-annotate": "^0.10.0",
    "copy-webpack-plugin": "^6.0.1",
    "cpx": "^1.5.0",
    "expose-loader": "^1.0.0",
    "file-loader": "^6.1.0",
